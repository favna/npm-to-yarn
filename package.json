{
  "name": "@armano/npm-to-yarn",
  "version": "1.1.1",
  "description": "Convert npm CLI commands to Yarn commands, and vice versa",
  "keywords": [
    "string convert",
    "documentation",
    "yarn to npm"
  ],
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "author": "Ben Gubler <nebrelbug@gmail.com>",
  "homepage": "https://github.com/armano2/npm-to-yarn#readme",
  "funding": "https://github.com/nebrelbug/npm-to-yarn?sponsor=1",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/armano2/npm-to-yarn.git"
  },
  "bugs": {
    "url": "https://github.com/armano2/npm-to-yarn/issues"
  },
  "license": "MIT",
  "engines": {
    "node": "^12.22.0 || ^14.17.0 || >=16.0.0"
  },
  "scripts": {
    "lint": "eslint src/*.ts test/*.spec.ts --ext .js,.ts",
    "build": "tsc -p tsconfig.json",
    "test": "jest --coverage",
    "test:watch": "jest --coverage --watch",
    "format": "prettier --write \"**/*.{ts,json,md}\""
  },
  "devDependencies": {
    "@types/jest": "^27.0.3",
    "@types/node": "^16.11.10",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "eslint": "^8.3.0",
    "eslint-config-prettier": "^8.3.0",
    "jest": "^27.3.1",
    "prettier": "^2.4.1",
    "ts-jest": "^27.0.7",
    "typescript": "^4.5.2"
  }
}
